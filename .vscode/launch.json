{
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Core Launch (console)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      // If you have changed target frameworks, make sure to update the program path.
      "program": "${workspaceFolder}/bin/Debug/net6.0/coryknox.dev.dll",
      "args": ["preview"],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "stopAtEntry": false,
      "requireExactSource": false,
      "justMyCode": false,
      "sourceLinkOptions": {
        "*": {
          "enabled": true
        }
      },
      "symbolOptions": {
        "searchPaths": [],
        "searchMicrosoftSymbolServer": true,
        "searchNuGetOrgSymbolServer": true
      }
    },
    {
      "name": "Preview (cake)",
      "type": "coreclr",
      "request": "launch",
      "program": "dotnet",
      "args": ["cake"],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "preLaunchTask": "toolsRestore",
      "stopAtEntry": false,
      "justMyCode": false,
      "sourceLinkOptions": {
        "*": {
          "enabled": true
        }
      },
      "symbolOptions": {
        "searchPaths": [],
        "searchMicrosoftSymbolServer": true,
        "searchNuGetOrgSymbolServer": true
      }
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach",
      "justMyCode": false,
      "sourceLinkOptions": {
        "*": {
          "enabled": true
        }
      },
      "symbolOptions": {
        "searchPaths": [],
        "searchMicrosoftSymbolServer": true,
        "searchNuGetOrgSymbolServer": true
      }
    }
  ]
}
